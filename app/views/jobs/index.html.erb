All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul>
    <%= link_to job.title, company_job_path(@company, job) %> at <%= job.company.name %><br>
    <%= link_to "Edit", edit_company_job_path(@company, job) %>
    <%= link_to "Delete", company_job_path(@company, job), method: :delete %>
  </ul>
<% end %>

<h3>Contacts</h3>
<%= render partial: 'companies/contact', collection: @company.contacts.reverse %>
<%= render partial: 'contacts/form' %>
