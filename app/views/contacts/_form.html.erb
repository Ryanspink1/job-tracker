<h3>Create a new contact</h3>
<%= form_for [@company, @contact] do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :position %>
    <%= f.text_field :position %>
  </p>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Submit' %>
  <p>
<% end %>
